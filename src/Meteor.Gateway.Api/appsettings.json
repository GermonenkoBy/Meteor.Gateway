{
  "Logging": {
    "LogLevel": {
      "Default": "Error"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "IncludeScopes": false,
        "UseUtcTimestamp": true,
        "TimestampFormat": "dd-MM-yyyy HH:mm:ss "
      }
    }
  },
  "AllowedHosts": "*"
}
